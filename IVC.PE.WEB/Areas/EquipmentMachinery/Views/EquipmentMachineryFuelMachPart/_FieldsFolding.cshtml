@model EquipmentMachineryFuelMachPartFoldingViewModel

<input asp-for="Id" hidden />

<input asp-for="EquipmentMachineryFuelMachPartId" hidden/>

<div class="form-group row">


    <div class="col-lg-12">
        <label asp-for="PartNumber" class="label"></label>
        <input asp-for="PartNumber" class="form-control" required />
    </div>

    <div class="col-lg-6">
        <label asp-for="PartDate" class="label"></label>
        <div class="input-group">
            <input asp-for="PartDate" class="form-control datepicker" required />
            <div class="input-group-append">
                <span class="input-group-text" id="basic-addon2">
                    <i class="fa fa-calendar"></i>
                </span>
            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <label asp-for="EquipmentMachineryOperatorId" class="label"></label>
        <input asp-for="EquipmentMachineryOperatorId" hidden />
        <select name="select_operator" class="form-control kt-select select2-operators" data-placeholder="Operador">
        </select>
    </div>
</div>
<div class="form-group row">

    <div class="col-lg-4">
        <label asp-for="FuelProviderId" class="label"></label>
        <input asp-for="FuelProviderId" hidden />
        <select name="select_fuel" class="form-control kt-select select2-fuels" data-placeholder="Distribuidor">
        </select>
    </div>

    <div class="col-lg-4">
        <label asp-for="FuelProviderFoldingId" class="label"></label>
        <input asp-for="FuelProviderFoldingId" hidden />
        <select name="select_plate" class="form-control kt-select select2-plates" data-placeholder="Placa Cisterna">
        </select>
    </div>

    <div class="col-lg-4">
        <label asp-for="FuelProviderPriceFoldingId" class="label"></label>
        <input asp-for="FuelProviderPriceFoldingId" hidden />
        <select name="select_price" class="form-control kt-select select2-prices" data-placeholder="Precio x galón">
        </select>
    </div>

    <div class="col-lg-4">
        <label asp-for="PartHour" class="label"></label>
        <input asp-for="PartHour" class="form-control" required />
    </div>
    <div class="col-lg-4">
        <label asp-for="Gallon" class="label"></label>
        <input asp-for="Gallon" class="form-control" required />
    </div>
    <div class="col-lg-4">
        <label asp-for="Horometer" class="label"></label>
        <input asp-for="Horometer" class="form-control" required />
    </div>


</div>



