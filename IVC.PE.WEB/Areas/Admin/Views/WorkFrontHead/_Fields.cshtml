@model WorkFrontHeadViewModel

<input asp-for="Id" hidden />

<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="ProjectId" class="label"></label>
        <select asp-for="ProjectId" class="form-control kt-select2 select2-projects" required data-placeholder="Proyecto"></select>
    </div>
</div>
<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="Code" class="label"></label>
        <input asp-for="Code" class="form-control" required />
    </div>
</div>
<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="UserId" class="label"></label>
        <select asp-for="UserId" class="form-control kt-select2 select2-users" required data-placeholder="Usuario"></select>
    </div>
</div>