@model ProjectCollaboratorViewModel

<input asp-for="Id" hidden />

<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="ProjectId" class="label"></label>
        <select asp-for="ProjectId" class="form-control kt-select2 select2-projects" required data-placeholder="Proyecto"></select>
    </div>
</div>
<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="ProviderId" class="label"></label>
        <select asp-for="ProviderId" class="form-control kt-select2 select2-providers" required data-placeholder="Proveedor"></select>
    </div>
</div>
<div class="form-group row">
    <div class="col-lg-6">
        <label asp-for="PaternalSurname" class="label"></label>
        <input asp-for="PaternalSurname" class="form-control" required />
    </div>
    <div class="col-lg-6">
        <label asp-for="MaternalSurname" class="label"></label>
        <input asp-for="MaternalSurname" class="form-control" required />
    </div>
</div>
<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="Name" class="label"></label>
        <input asp-for="Name" class="form-control" required />
    </div>
</div>
