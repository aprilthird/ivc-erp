@model RacsSummaryViewModel
<input asp-for="Id" hidden />

<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="ProjectId" class="label"></label>
        <input asp-for="ProjectId" hidden />
        <select name="select_project" class="form-control kt-select select2-projects" data-placeholder="Proyecto"></select>
    </div>
</div>

<div class="form-group row">
    <div class="col-lg-4">
        <label asp-for="RacsCode" class="label"></label>
        <input asp-for="RacsCode" class="form-control" />
    </div>
    <div class="col-lg-8">
        <label asp-for="VersionCode" class="label"></label>
        <input asp-for="VersionCode" class="form-control" />
    </div>
</div>
