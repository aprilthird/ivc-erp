    <div class="col-lg-4">
        <label asp-for="SewerBoxEnd.CoverLevel" class="label"></label>
        <input asp-for="SewerBoxEnd.CoverLevel" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxEnd.ArrivalLevel" class="label"></label>
        <input asp-for="SewerBoxEnd.ArrivalLevel" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxEnd.BottomLevel" class="label"></label>
        <input asp-for="SewerBoxEnd.BottomLevel" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxEnd.TerrainType" class="label"></label>
        <select asp-for="SewerBoxEnd.TerrainType" class="form-control kt-select select2-terrain-types" data-placeholder="Tipo de Terreno">
            @foreach (var item in ConstantHelpers.Terrain.Type.NAMES)
            {
                <option value="@item.Key">@item.Value</option>
