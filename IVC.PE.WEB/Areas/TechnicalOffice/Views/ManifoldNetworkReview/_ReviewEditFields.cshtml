@model SewerManifoldViewModel

<input asp-for="Id" hidden />

<div class="form-group row">
    <div class="col-lg-12">
        <h6>Buzón Aguas Arriba - <span id="i-name"></span></h6>
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxStart.Code" class="label"></label>
        <input asp-for="SewerBoxStart.Code" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxStart.Thickness" class="label"></label>
        <input asp-for="SewerBoxStart.Thickness" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxStart.CoverLevel" class="label"></label>
        <input asp-for="SewerBoxStart.CoverLevel" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxStart.ArrivalLevel" class="label"></label>
        <input asp-for="SewerBoxStart.ArrivalLevel" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxStart.BottomLevel" class="label"></label>
        <input asp-for="SewerBoxStart.BottomLevel" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxStart.TerrainType" class="label"></label>
        <select asp-for="SewerBoxStart.TerrainType" class="form-control kt-select select2-terrain-types" data-placeholder="Tipo de Terreno">
            @foreach (var item in ConstantHelpers.Terrain.Type.NAMES)
            {
                <option value="@item.Key">@item.Value</option>
            }
        </select>
    </div>
    <div class="col-lg-4">
        <label asp-for="SewerBoxStart.SewerBoxType" class="label"></label>
        <select asp-for="SewerBoxStart.SewerBoxType" class="form-control kt-select select2-sewerbox-types" data-placeholder="Tipo de Buzón">
            @foreach (var item in ConstantHelpers.Sewer.Box.Type.VALUES)
            {
                <option value="@item.Key">@item.Value</option>
            }
        </select>
    </div>
</div>

<div class="form-group row">
    <div class="col-lg-12">
        <h6>Buzón Aguas Abajo - <span id="j-name"></span></h6>
