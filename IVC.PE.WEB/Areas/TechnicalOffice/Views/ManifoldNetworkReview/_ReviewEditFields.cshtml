        </select>
    </div>
</div>

<div class="form-group row">
    <div class="col-lg-3">
        <label asp-for="Pavement2In" class="label"></label>
        <input type="number" asp-for="Pavement2In" class="form-control" />
    </div>
    <div class="col-lg-3">
        <label asp-for="Pavement3In" class="label"></label>
