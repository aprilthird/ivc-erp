            @foreach (var item in ConstantHelpers.Sewer.Box.Type.VALUES)
            {
                <option value="@item.Key">@item.Value</option>
            }
        </select>
    </div>
</div>

<div class="form-group row">
    <div class="col-lg-12">
        <h6>Tramo - <span id="manifold_name"></span></h6>
    </div>
    <div class="col-lg-4">
        <label asp-for="Name" class="label"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="col-lg-4">
        <label asp-for="Address" class="label"></label>
        <input asp-for="Address" class="form-control" />
    </div>
    <div class="col-lg-4">
