
@{
    ViewData["Title"] = "Reportes de Costos y Horas Hombre";
}

<div class="kt-portlet kt-portlet--tabs">
    <div class="kt-portlet__head">
        <div class="kt-portlet__head-label">
            <span class="kt-portlet__head-icon">
                <i class="flaticon-list-2"></i>
            </span>
            <h3 class="kt-portlet__head-title">
                @ViewBag.Title
            </h3>
        </div>
    </div>

    <div class="kt-portlet__body">
        <div class="form-group row">
            <partial name="_SearchFields" />
        </div>
        <div class="form-group row">
            <div class="col-lg-12">
                <a id="payrollWorkforceCostBtn" href="#" class="btn btn-outline-success">
                    <i class="flaticon2-list"></i> Generar Excel Costos de la Planilla
                </a>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-lg-12">
                <a id="payrollPhaseCostBtn" href="#" class="btn btn-outline-success">
                    <i class="flaticon2-list"></i> Generar Excel Costos de la Planilla por Fase
                </a>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-lg-12">
                <a id="payrollWorkforceHoursCostsBtn" href="#" class="btn btn-outline-success">
                    <i class="flaticon2-list"></i> Generar Excel Horas y Costos de la Planilla
                </a>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-lg-12">
                <a id="payrollWorkforcePhasesWorkersBtn" href="#" class="btn btn-outline-success">
                    <i class="flaticon2-list"></i> Generar Excel Obreros por Fase
                </a>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/js/areas/human-resources/worker-payroll-report/index.js"></script>
}
