@model WorkerDailyTasksImportDayViewModel

<input asp-for="ProjectId" hidden />

<div class="form-group row">
    <div class="col-md-8">
        <label asp-for="WorkFrontHeadId" class="col-form-label"></label>
        <select asp-for="WorkFrontHeadId" class="form-control kt-select2 select2-work-front-heads" required>
            <option>--Seleccione un Jefe de Frente--</option>
        </select>
    </div>
    <div class="col-md-4">
        <label asp-for="DateTask" class="col-form-label"></label>
        <div class="input-group">
            <input asp-for="DateTask" class="form-control datepicker" required />
            <div class="input-group-append">
                <span class="input-group-text" id="basic-addon2">
                    <i class="fa fa-calendar"></i>
                </span>
            </div>
        </div>
    </div>
</div>
<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="File" class="col-form-label"></label>
        <div class="custom-file">
            <input asp-for="File" class="custom-file-input" name="file" required>
            <label class="custom-file-label" for="customFile">Escoge un archivo</label>
        </div>
    </div>
</div>
