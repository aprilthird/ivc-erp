@model ReEntryForReturnViewModel

<input asp-for="Id" hidden />

<div class="form-group row">
    <div class="col-lg-6">
        <label asp-for="ProjectFormulaId" class="label"></label>
        <input asp-for="ProjectFormulaId" hidden />
        <select name="select_project_formula" class="form-control kt-select select2-project-formulas"></select>
    </div>
    <div class="col-lg-6">
        <label asp-for="WorkFrontId" class="label"></label>
        <input asp-for="WorkFrontId" hidden />
        <select name="select_work_front" class="form-control kt-select select2-work-fronts"></select>
    </div>
</div>

<div class="form-group row">
    <div class="col-lg-5">
        <label asp-for="SewerGroupId" class="label"></label>
        <input asp-for="SewerGroupId" hidden />
        <select name="select_sewer_group" class="form-control kt-select select2-sewer-groups"></select>
    </div>
    <div class="col-lg-4">
        <label asp-for="SupplyFamilyId" class="label"></label>
        <input asp-for="SupplyFamilyId" hidden />
        <select name="select_supply_family" class="form-control kt-select select2-supply-families"></select>
    </div>
    <div class="col-lg-3">
        <label asp-for="ReturnDate" class="label"></label>
        <div class="input-group">
            <input asp-for="ReturnDate" class="form-control datepicker" required />
            <div class="input-group-append">
                <span class="input-group-text" id="basic-addon2">
                    <i class="fa fa-calendar"></i>
                </span>
            </div>
        </div>
    </div>
</div>


<div class="form-group">
    <label asp-for="File" class="label">Archivo:</label>
    <div class="custom-file">
        <input asp-for="File" type="file" class="custom-file-input" />
        <label class="custom-file-label" for="customFile">Escoge un archivo</label>
    </div>
</div>