@model SupplierCatalogViewModel

@Html.HiddenFor(model => model.Id)
<input asp-for="SupplierGuid" hidden value="@ViewBag.SupplierGuid" />
<input asp-for="FileType" hidden value="@ConstantHelpers.TechnicalLibrary.FileType.SUPPLIER_CATALOG" />
<div class="form-group">
    <label asp-for="Title" class="label"></label>
    <input asp-for="Title" class="form-control" required />
</div>
<div class="form-group">
    <label asp-for="File" class="label">Archivo:</label>
    <div class="custom-file">
        <input asp-for="File" type="file" class="custom-file-input" />
        <label class="custom-file-label" for="customFile">Escoge un archivo</label>
    </div>
</div>
