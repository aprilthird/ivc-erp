@model SupplierCatalogViewModel

@Html.HiddenFor(model => model.SupplierViewModel.Id)
<input asp-for="SupplierViewModel.Id" hidden value="@ViewBag.SupplierGuid" />
<input asp-for="SupplierViewModel.FileCount" hidden/>
<div class="form-group">
    <label asp-for="SupplierViewModel.RUC" class="label"></label>
    <input asp-for="SupplierViewModel.RUC" class="form-control" required />
</div>
<div class="form-group">
    <label asp-for="SupplierViewModel.BusinessName" class="label"></label>
    <input asp-for="SupplierViewModel.BusinessName" class="form-control" required />
</div>