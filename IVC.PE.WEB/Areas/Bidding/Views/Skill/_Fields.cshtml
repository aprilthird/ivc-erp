@model IVC.PE.WEB.Areas.Bidding.ViewModels.SkillViewModels.SkillViewModel
<input asp-for="Id" hidden />
<input asp-for="SkillRenovation.Id" hidden />

<div class="form-group row">

    <div class="col-lg-8">
        <label asp-for="ProfessionalId" class="label"></label>
        <input asp-for="ProfessionalId" hidden />
        <select name="select_professional" class="form-control kt-select select2-professionals" data-placeholder="Profesionales"></select>
    </div>

</div>

<div class="form-group row">

    <div class="col-lg-6">
        <label asp-for="SkillRenovation.CreateDate" class="label"></label>
        <div class="input-group">
            <input asp-for="SkillRenovation.CreateDate" class="form-control datepicker" required />
            <div class="input-group-append">
                <span class="input-group-text" id="basic-addon2">
                    <i class="fa fa-calendar"></i>
                </span>
            </div>
        </div>
    </div>

    <div class="col-lg-6">
        <label asp-for="SkillRenovation.EndDate" class="label"></label>
        <div class="input-group">
            <input asp-for="SkillRenovation.EndDate" class="form-control datepicker" required />
            <div class="input-group-append">
                <span class="input-group-text" id="basic-addon2">
                    <i class="fa fa-calendar"></i>
                </span>
            </div>
        </div>
    </div>

    @*<div class="col-lg-4">
        <label asp-for="SkillRenovation.IsTheLast" class="label"></label>
        <input asp-for="SkillRenovation.IsTheLast" hidden />
        <select name="select_isthelast" class="form-control kt-select select2-isthelast" required>
            <option value="false" selected>No</option>
            <option value="true">Sí</option>
        </select>
    </div>*@

    <div class="col-lg-12">
        <label asp-for="SkillRenovation.File" class="label"></label>
        <div class="custom-file">
            <input asp-for="SkillRenovation.File" type="file" class="custom-file-input" />
            <label class="custom-file-label" for="customFile">Escoge un archivo</label>
        </div>
    </div>

</div>




