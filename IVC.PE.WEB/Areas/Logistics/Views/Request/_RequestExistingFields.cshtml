@model ExistingRequestViewModel

<div class="form-group row">
    <div class="col-md-4">
        <label class="col-form-label">Tipo de Solicitud</label>
        <select id="type_filter" class="form-control kt-select2 select2-types">
            <option>Todos</option>
            @foreach (var item in ConstantHelpers.Logistics.RequestOrder.Type.VALUES)
            {
                <option value="@item.Key">@item.Value</option>
            }
        </select>
    </div>
    <div class="col-lg-12">
        <label asp-for="PreRequestIds" class="label"></label>
        <select asp-for="PreRequestIds" multiple class="form-control kt-select2 select2-pre-requests"></select>
    </div>
    <div class="col-lg-4">
        <label class="label">Tipos de Almacén</label>
        <select class="form-control kt-select2 select2-warehouses-types"></select>
    </div>
    <div class="col-lg-4">
        <label asp-for="WarehouseId" class="label"></label>
        <select asp-for="WarehouseId" class="form-control kt-select2 select2-warehouses"></select>
    </div>
</div>

