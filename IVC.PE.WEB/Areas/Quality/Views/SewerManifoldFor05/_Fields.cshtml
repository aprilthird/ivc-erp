@model SewerManifoldFor05ViewModel

<input asp-for="Id" hidden />

<div class="form-group row">
    <div class="col-lg-12">
        <label asp-for="SewerManifoldId" class="label"></label>
        <input asp-for="SewerManifoldId" hidden />
        <select name="select_manifold" class="form-control kt-select select2-manifolds"></select>
    </div>
    <div class="col-lg-4">
        <button id="SearchSelectManifold" type="button" class="search-manifold btn btn-secondary">Buscar</button>
    </div>
</div>

<div class="form-group">
    <label class="label">Cuadrilla</label>
    <input id="SewerGroup_Code" class="form-control" disabled="disabled" />
</div>

<div class="form-group row">
    <div class="col-lg-6">
        <label asp-for="CertificateNumber" class="label">N° Certificado</label>
        <input asp-for="CertificateNumber" class="form-control" />
    </div>
    <div class="col-lg-6">
        <label asp-for="ShippingDate" class="label">Fecha de Envío</label>
        <div class="input-group">
            <input asp-for="ShippingDate" class="form-control datepicker" />
            <div class="input-group-append">
                <span class="input-group-text" id="basic-addon2">
                    <i class="fa fa-calendar"></i>
                </span>
            </div>
        </div>
    </div>
</div>

<div class="form-group">
    <label asp-for="File" class="label">Archivo:</label>
    <div class="custom-file">
        <input asp-for="File" type="file" class="custom-file-input" />
        <label class="custom-file-label" for="customFile">Escoge un archivo</label>
    </div>
</div>