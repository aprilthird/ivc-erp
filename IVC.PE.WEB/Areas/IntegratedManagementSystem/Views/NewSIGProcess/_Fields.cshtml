        <label asp-for="UserId" class="label"></label>
        <input asp-for="UserId" hidden />
        <select name="select_user" class="form-control kt-select select2-users"></select>
    </div>
</div>

<div class="form-group row">
    <div class="col-lg-6">
        <label asp-for="ProcessName" class="label"></label>
        <input asp-for="ProcessName" class="form-control" />
    </div>
    <div class="col-lg-6">
        <label asp-for="Code" class="label"></label>
        <input asp-for="Code" class="form-control" />
    </div>
</div>