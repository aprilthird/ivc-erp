<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IVC.PE.APP.Views.EquipmentMachinery.EquipmentMachPartCheckPage"
             BindingContext="{Binding Main, Source={StaticResource Locator}}"
             Title="Consultar QR">
    <ContentPage.Content>
        <StackLayout
            VerticalOptions="FillAndExpand"
            HorizontalOptions="FillAndExpand"
            BindingContext="{Binding EquipmentMachPartCheckViewModel}"
            Padding="8">

            <Button
                HorizontalOptions="FillAndExpand"
                Command="{Binding CaptureQrCommand2}"
                Text="Capturar Código QR Operador (1)"/>
            
            <Button
                HorizontalOptions="FillAndExpand"
                Command="{Binding CaptureQrCommand}"
                Text="Capturar Código QR Maquina (2)"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>